pipeline{
    agent any
    environment {
		PATH = "/opt/maven/bin:$PATH"
	}
	stages {
		stage("Git Checout") {
			steps {
				git branch: 'main', credentialsId: '9dcc1970-67f8-489a-aa7f-7db1a7ac8074', url: 'https://github.com/mugun44/maven'
			}
		}
		stage("Maven-Build") {
			steps {
				sh "mvn clean package"
				sh "mv target/*.war target/myweb.war"
			}
		}
}
}
